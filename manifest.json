{
  "manifest_version": 3,
  "name": "Ivey Learn Plus",
  "description": "Extended functions for Ivey Learn",
  "version": "1.0",
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://ivey.instructure.com/*"
      ],
      "match_origin_as_fallback": true,
      "all_frames": true
    }
  ]
}
